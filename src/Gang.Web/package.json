{
  "name": "gang-web",
  "private": true,
  "version": "0.0.1",
  "description": "Gang Demo Chat App",
  "license": "MIT",
  "scripts": {
    "dev": "stencil build --dev --watch",
    "build": "npm run build.icons && stencil build",
    "build.icons": "node build.icons.js",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "local": "npm i file:../GangJs/Core",
    "remote": "npm un @gang-js/core && npm i @gang-js/core && npm audit fix"
  },
  "dependencies": {
    "@gang-js/core": "^2.0.0-beta.7",
    "rxjs": "^6.6.3"
  },
  "devDependencies": {
    "@stencil/core": "^2.0.3",
    "@stencil/router": "^1.0.1",
    "@trust/webcrypto": "^0.9.2",
    "@types/jest": "26.0.14",
    "@types/puppeteer": "3.0.2",
    "convert-svg-to-png": "^0.5.0",
    "imagemin": "^7.0.1",
    "imagemin-pngquant": "^9.0.1",
    "jest": "26.4.2",
    "jest-cli": "26.4.2",
    "puppeteer": "5.3.1",
    "to-ico": "^1.1.5",
    "typescript": "^4.0.3",
    "workbox-build": "5.1.4"
  }
}
