{
  "App": {
    "Id": "uk.co.antix.gang",
    "Name": "Gang Demo",
    "RootUrl": "http://gang.antix.co.uk"
  },
  "Auth": {
    "Secret": "{{Auth.Secret}}",
    "Enabled": true
  },
  "Smtp": {
    "Host": "localhost",
    "Port": 25,
    "FromAddress": "bot@antix.co.uk"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    },
    "File": {
      "RollingInterval": "Day"
    }
  }
}
